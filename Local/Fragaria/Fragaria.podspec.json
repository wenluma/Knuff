{
  "name": "Fragaria",
  "version": "1.0.1",
  "summary": "Cocoa syntax highlighting text view.",
  "homepage": "http://www.mugginsoft.com/code/fragaria",
  "authors": {
    "Jonathan Mitchell": "jonathan@mugginsoft.com"
  },
  "source": {
    "git": "https://github.com/explicitcall/Fragaria.git",
    "tag": "1.0.1"
  },
  "source_files": [
    "*.h",
    "NS*.{h,m}",
    "MG*.{h,m}",
    "ICU*.{h,m}",
    "SML*.{h,m}",
    "DB*.{h,m}",
    "FRAFont*.{h,m}"
  ],
  "resources": [
    "SML*.xib",
    "Syntax Definitions/*.plist",
    "SyntaxDefinitions.plist"
  ],
  "public_header_files": "*.h",
  "platforms": {
    "osx": "10.10"
  },
  "libraries": "icucore",
  "license": "BSD",
  "requires_arc": true,
  "subspecs": [
    {
      "name": "unicode",
      "header_dir": "unicode",
      "source_files": "icu/**/*.h"
    }
  ]
}
